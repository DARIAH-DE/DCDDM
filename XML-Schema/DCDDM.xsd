    <xs:element name="adress">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Definition: Contains information related to a post address. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:street" minOccurs="0"/>                    
                <xs:element ref="dclap:houseNumber" minOccurs="0"/>
                <xs:element ref="dclap:postalCode" minOccurs="0"/>
                <xs:element ref="dclap:place" minOccurs="0"/>
                <xs:element ref="dclap:country" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="name" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Definition: The name of the described entity (e.g. a person or an organisation).
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: Should contain a person´s last or family name (surname) or the name of the described organisation.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="street" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Definition: The street´s name of post adress.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="houseNumber" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Definition: The house number of the agent´s post adress.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="postalCode" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Definition: The postal code of the agent´s post adress. 
            </xs:documentation>
            <xs:documentation>
                Comment: No use of country prefixes foreseen, e.g. use 38304 instead of D-38304.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="place">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Definition: Holds information related to the place (city). 
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: The place should be linked to some normdata and provided with a human readable label. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:spatial"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="country">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Definition: Holds information related to the country. 
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: The place should be linked to some normdata and provided with a human readable label. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:spatial"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="agentType" type="dclap:agentTypeValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/elements/1.1/type">
                Definition: The nature or genre of the resource.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: Use of standard vocabulary required. The provided values are 'person' and 'organisation'
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="serviceAccessMethod" type="dclap:servicaAccessMethodValues"/>
    <xs:element name="serviceFunction" type="dclap:serviceFunctionValues"/>
    <xs:element name="serviceURL" type="xs:anyURI"/>
