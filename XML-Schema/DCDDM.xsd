<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:dclap="http://schema.dariah.eu/colreg/dclap/" xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:cld="http://purl.org/cld/terms/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:mods="http://www.loc.gov/mods/v3" xmlns:dcldf="http://purl.org/cld/freq"
    xmlns:vcard="http://www.w3.org/2006/vcard/"
    targetNamespace="http://schema.dariah.eu/colreg/dclap/" elementFormDefault="qualified"
    attributeFormDefault="unqualified">

<!--
        NOTE: "source="DCLAP Data Model" is a placeholder for an upcoming online-documentation of the DCLAP Data Model
        NOTE: The prefix dclap: is considered as a working title and stands for Dariah Collection Application Profile    
-->
    <xs:annotation>
        <xs:documentation xml:lang="en">
            Definition: A XML-Schema for collection descriptions. 
            Principles: As strict as possible. Avoiding dependencies by avoiding the import of other XML-Schemas.
            History: Based on the Dublin Core Collection Application Profile; the DARIAH-DE Collection Registry (GUI) and an internal DARIAH-DE paper/wiki-page "DARIAH-DE Collection Level Description Application
            Profile" 
            Creators: Peter Andorfer and David Maus
        </xs:documentation>
    </xs:annotation>
    <xs:import namespace="http://www.w3.org/XML/1998/namespace"
        schemaLocation="http://www.w3.org/2001/03/xml.xsd"/>

    <xs:element name="CollectionDescription">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Definition: Root element of the collection description
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:COLLECTION" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <!--
    #############################################################
    #                    CLASS-DEFINITIONS                    #
    #############################################################
-->
    
    <xs:element name="COLLECTION">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/dcmitype/Collection">
                Definition:	An aggregation of resources.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/dcmitype/Collection">
                Comment: A collection is described as a group; its parts may also be separately described.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:title" minOccurs="1"/>
                <xs:element ref="dclap:identifier" minOccurs="1" maxOccurs="unbounded"/>
                <xs:element ref="dclap:landingPage" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dclap:collectionType" minOccurs="1"/>
                <xs:element ref="dclap:description" minOccurs="1" maxOccurs="unbounded"/>
                <xs:element ref="dclap:acronym" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dclap:size" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:language" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:itemType" minOccurs="1" maxOccurs="unbounded"/>
                <xs:element ref="dclap:collectionDescriptionRights" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="dclap:itemsRights" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:accessRights" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="dclap:accrualMethod" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:accrualPeriodicity" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:accrualPolicy" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:provenance" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:audience" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:subject" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:spatial" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:temporal" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:collectionCreated" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:dateItemsCreated" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:creator" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:owner" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:isLocatedAt" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:isAccessedVia" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:hasPartOf" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:isPartOf" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:associatedCollection" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:metadataEncodingScheme" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dclap:itemEncodingScheme" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="AGENT" type="dclap:AGENT">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/Agent">
                Definition: A resource that acts or has the power to act.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/Agent">
                Comment: Examples of Agent include person, organization, and software agent.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="SERVICE" type="dclap:SERVICE">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/dcmitype/Service">
                Definition:	A system that provides one or more functions.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/dcmitype/Service">
                Comment: Examples include a photocopying service, interlibrary loans, a reading room, a metadata harvesting protocoll (OAI-PMH), an OPAC, a GND-Beacon, etc.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="LOCATION" type="dclap:LOCATION">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/Location">
                Definition:	A spatial region or named place.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
   
    <!--
    #############################################################
    #                    ELEMENT-DEFINITIONS                    #
    #############################################################
-->
    
    <xs:element name="title" type="dclap:simpleLiteral">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/elements/1.1/title">
                Definition: A name given to the resource.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctitle">
                Comment: A value string must be provided.                
                To record titles in multiple languages, use multiple statements and in each statement associate the value string with the appropriate language tag. 
                Where an existing name is used, the value string should preserve the original wording, order and spelling of an existing name. Punctuation need not reflect the usage of the original. Subtitles should be separated from the title by a sequence of space-colon-space.           
            </xs:documentation>
            <xs:documentation xml:lang="en">
                Supplied Comment: The usage of several different languages in one titel element should be avoided if possible. 
            </xs:documentation>
            <xs:documentation xml:lang="deu" source="DCLAP Data Model">
                Don´t: International Dunhuang Project. Die Seidenstraße online
                Do: Internationales Dunhuang Projekt: Die Seidenstraße online 
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="identifier" type="dclap:identifier">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/elements/1.1/identifier">
                Definition: An unambiguous reference to the resource within a given context.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/elements/1.1/identifier">
                Comment: Recommended best practice is to identify the resource by means of a string conforming to a formal identification system. 
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="landingPage" type="xs:anyURI">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Definition: URL to a webpage of the collection.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="collectionType" type="dclap:collectionTypeValues">
        <xs:annotation>
            <xs:documentation source="http://purl.org/dc/elements/1.1/type">
                Definition: The nature or genre of the resource.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: Use of standard vocabulary required. Provided values are based on DARIAH-DE suggestions.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="description" type="dclap:simpleLiteral">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermsabstract">
                Definition: A free text summary description of the collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermsabstract">
                Comment: A value string must be provided.              
                To record descriptions in multiple languages, use multiple statements and in each statement associate the value string with the appropriate language tag.
                Although a description might contain detailed subject-specific information, at least part of the description should be understandable by an end-user with no specialist knowledge of the subject area.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="acronym" type="dclap:simpleLiteral">  
        <xs:annotation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Defintion: An acronym of the collection.  
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="size" type="dclap:simpleLiteral">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/extent">
                Definition: The size of the collection.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="language" type="dclap:languageValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/elements/1.1/language">
                Definition: A language of the resource.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="itemType" type="dclap:itemTypeValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/cld/terms/itemType">
                Definition: The nature or genre of the content of one or more items within the collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/documents/dcmi-type-vocabulary/#H7">
                Comment: Use of standard vocabulary required. Values taken from DCMI Type Vocabulary.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="collectionDescriptionRights" type="dclap:rigthsValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/elements/1.1/rights">
                Definition: A statement of any rights held in/over the collection description.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="https://dev2.dariah.eu/wiki/x/cg9FAg">
                Comment: Use of standard vocabulary required. Provided values are based on DARIAH-DE suggestions.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="itemsRights" type="dclap:rigthsValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/elements/1.1/rights">
                Definition: A statement of any rights held in/over the collection´s items.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="https://dev2.dariah.eu/wiki/x/cg9FAg">
                Comment: Use of standard vocabulary required. Provided values are based on DARIAH-DE suggestions.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="accessRights" type="dclap:accessRightsValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/accessRights">
                Definition: Information about who can access the resource or an indication of its security status.
            </xs:documentation>
            <xs:documentation xml:lang="de" source="DCLAP Data Model">
                Comment: Use of standard vocabulary required. Provided values are based on DARIAH-DE suggestions. 
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="accrualMethod" type="dclap:accrualMethodsValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/accrualMethod">
                Definition: The method by which items are added to a collection.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="accrualPeriodicity" type="dclap:accrualPeriodicityValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="The frequency with which items are added to a collection.">
                Definition: The frequency with which items are added to a collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: Use of standard vocabulary required. Provided values are based on DARIAH-DE suggestions.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="accrualPolicy" type="dclap:accrualPolicyValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/accrualPolicy">
                Definition: The policy governing the addition of items to a collection.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="provenance" type="dclap:simpleLiteral">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/provenance">
                Definition: A statement of any changes in ownership and custody of the resource since its creation that are significant for its authenticity, integrity, and interpretation.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/provenance">
                Comment:The statement may include a description of any changes successive custodians made to the resource.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="audience" type="dclap:simpleLiteral">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/audience">
                Definition: A class of entity for whom the resource is intended or useful.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="subject">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/elements/1.1/subject">
                Definition:The topic of the resource.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: The subject has to be taken either from GND, DCC or LCSH.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:subjectValues" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation xml:lang="en" source="DCLAP Data Model">
                            Definition: Contains the code, signature, URI, etc. matching the chosen encodingScheme-attribute.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="dclap:label" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation xml:lang="en" source="DCLAP Data Model">
                            Definition: Provides a human readable text of the chosen subject.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="spatial">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/spatial">
                Definition: Spatial characteristics of the resource.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: Local entities should be linked either to GeoNames or Getty Thesaurus of Geographic Names and a human readable label should be provided.  
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:spatialValues" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="dclap:label" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="temporal">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/temporal">
                Definition: The temporal coverage of the content of the items in the collection..
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: Dates must following provided formatting rules. A human readable label can be added. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:temporalValue" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="dclap:label" minOccurs="1" maxOccurs="1"/>
            </xs:sequence> 
        </xs:complexType>
    </xs:element>
    <xs:element name="collectionCreated">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermscreated">
                Definition: A range of dates over which the collection was accumulated.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: Date must follow provided formatting rules. A human readable label can be added.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:temporalValue" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="dclap:label" minOccurs="1" maxOccurs="1"/>
            </xs:sequence> 
        </xs:complexType>
    </xs:element>
    <xs:element name="dateItemsCreated">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/cld/terms/dateItemsCreated">
                Definition: A range of dates over which the individual items within the collection were created.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: Dates must following provided formatting rules. A human readable label can be added. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:temporalValue" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="dclap:label" minOccurs="1" maxOccurs="1"/>
            </xs:sequence> 
        </xs:complexType>
    </xs:element>
    <xs:element name="creator">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/elements/1.1/creator">
                Definition: An entity primarily responsible for making the resource.</xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: A wrapper class for describing an AGENT. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:AGENT" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="owner">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Definition: An AGENT who owns the collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: A wrapper class for describing an AGENT. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:AGENT" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="isLocatedAt">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/cld/terms/isLocatedAt">
                Definition: A location where the collection is held.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: A wrapper class for describing a LOCATION.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:LOCATION" minOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="isAccessedVia">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/cld/terms/isAccessedVia">
                Definition: A service that provides access to the items within the collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: A wrapper class for describing a SERVICE.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:SERVICE" minOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="hasPartOf">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/hasPart">
                Definition:A related resource [Collection] that is included either physically or logically in the described resource.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: A wrapper class for describing a COLLECTION.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:COLLECTION"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="isPartOf">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/isPartOf">
                Definition: A related resource [Collection] in which the described resource is physically or logically included.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Comment: A wrapper class for describing a COLLECTION.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:COLLECTION"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="metadataEncodingScheme" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Definition: Encoding scheme for provided metadata of the collection´s objects.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="associatedCollection">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#colcldassociatedCollection">
                Definition: A second collection that is associated with the current collection.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dclap:COLLECTION"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="itemEncodingScheme" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="DCLAP Data Model">
                Definition: Encding scheme used for modelling the collection´s objects. 
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="spatialValues">
        <xs:complexType mixed="true">
            <xs:attribute name="vocabularyEncodingScheme" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="TGN">
                            <xs:annotation>
                                <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/TGN">
                                    Definition:	The set of places specified by the Getty Thesaurus of Geographic Names.  
                                </xs:documentation>
                                <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/TGN">
                                    See: http://www.getty.edu/research/tools/vocabulary/tgn/index.html
                                </xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="GeoNames">
                            <xs:annotation>
                                <xs:documentation xml:lang="en">
                                    Definition:	The set of places specified by GeoNames geographical database.
                                </xs:documentation>
                                <xs:documentation xml:lang="en">
                                    See: http://www.geonames.org/
                                </xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="GND">
                            <xs:annotation>
                                <xs:documentation xml:lang="en">
                                    Definition:	The set of places specified by the Deutschen Nationalbibliothek.
                                </xs:documentation>
                                <xs:documentation xml:lang="en">
                                    See: https://portal.dnb.de
                                </xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="subjectValues">
        <xs:complexType mixed="true">
            <xs:attribute name="vocabularyEncodingScheme" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
