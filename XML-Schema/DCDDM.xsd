            <xs:pattern value="[\d{,4}][-\d\d]+[-\d\d]+(/|--)[\d{,4}][-\d\d]+[-\d\d]+"/>
            <xs:pattern value="[\d{,4}][-\d\d]+[-\d\d]+"></xs:pattern>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="spatialValues">
        <!-- dmaus: Keine separaten Elemente, sondern Attribut "encodingScheme" -->
        <!-- dmaus: Attribut ist optional, wenn nicht verwendet dann Freitext -->
        <!-- dmaus: Notwendigkeit eines Label ist zu diskutieren -->
        <xs:sequence>
            <xs:element name="geonames" minOccurs="0" maxOccurs="unbounded">
