    <xs:element name="schemaID" type="xs:string"/>
    <xs:element name="serviceInterface" type="xs:anyURI"/>
    <xs:element name="serviceHelpURL" type="xs:anyURI"/>
    <xs:element name="accessControl" type="dclap:accessRightsValues"/>
