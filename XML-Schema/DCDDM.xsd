            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="dclap:title" minOccurs="1" maxOccurs="1"/>
            <xs:element ref="dclap:identifier" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="dclap:acronym" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="dclap:serviceAccessMethod" minOccurs="1"/>
            <xs:element ref="dclap:serviceFunction" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="dclap:serviceURL" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="dclap:subset" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="dclap:schemaID" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="dclap:serviceInterface" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="dclap:serviceHelpURL" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="dclap:accessControl" minOccurs="1"/>
            <xs:element ref="dclap:consultant" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="LOCATION">
        <xs:annotation>
            <xs:documentation>Definition: Datatype for the dclap:LOCATION-Element.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="dclap:name" minOccurs="1"/>
            <xs:element ref="dclap:spatial" minOccurs="1"/>
