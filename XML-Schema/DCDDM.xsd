    <xs:complexType name="AGENT">
        <xs:sequence>
            <xs:element ref="dclap:agentType" minOccurs="1"/>
            <xs:element ref="dclap:name" minOccurs="1"/>
            <xs:element name="firstName" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en" source="DCLAP Data Model">
                        Definition: The first name of a person.
                    </xs:documentation>
                    <xs:documentation xml:lang="en" source="DCLAP Data Model">
                        Comment: Only used for persons.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dclap:identifier" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="dclap:adress" minOccurs="0" maxOccurs="1"/>
            <xs:element name="email" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="webpage" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="tel" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="identifier">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="vocabularyEncodingScheme" use="required">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="GND"/>
                            <xs:enumeration value="other"/>
