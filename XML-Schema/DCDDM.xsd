            <xs:enumeration value="Attribution Non-Commercial Share Alike v1.0(CC BY-NC-SA)"/>
            <xs:enumeration value="Attribution Non-Commercial No Derivatives v1.0(CC BY-NC-ND)"/>
            <xs:enumeration value="Attribution v2.0(CC BY)"/>
            <xs:enumeration value="Attribution Share Alike v2.0(CC BY-SA)"/>
            <xs:enumeration value="Attribution No Derivatives v2.0(CC BY-ND)"/>
