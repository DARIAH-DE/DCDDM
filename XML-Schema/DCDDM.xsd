<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
    xmlns:dcddm="http://schema.dariah.eu/colreg/dcddm/"
    targetNamespace="http://schema.dariah.eu/colreg/dcddm/" elementFormDefault="qualified"
    attributeFormDefault="unqualified">

<!--
        NOTE: xmlns:dcddm="http://schema.dariah.eu/colreg/dcddm/" is a placeholder for an upcoming online-documentation of the dcddm Data Model
        NOTE: The prefix dcddm: is considered as a working title and stands for DARIAH Collection Description Data Model   
-->
    <xs:annotation>
        <xs:documentation xml:lang="en">
            Definition: A XML-Schema for collection descriptions. 
            Principles: As strict as possible. Avoiding dependencies by avoiding the import of other XML-Schemas.
            History: Based on the Dublin Core Collection Application Profile; the DARIAH-DE Collection Registry (GUI) and an internal DARIAH-DE paper/wiki-page "DARIAH-DE Collection Level Description Application
            Profile" 
            Creator: Peter Andorfer
            See: https://github.com/DARIAH-DE/DCDDM
        </xs:documentation>
    </xs:annotation>
    
    <xs:import namespace="http://www.w3.org/XML/1998/namespace"
        schemaLocation="http://www.w3.org/2001/03/xml.xsd"/>
        
    <!--
    #############################################################
    #                    CLASS-DEFINITIONS                    #
    #############################################################
-->
    
    <xs:element name="COLLECTION">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/dcmitype/Collection">
                Definition:	An aggregation of resources.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/dcmitype/Collection">
                Comment:In the DARIAH Collection Description Data Model a dcddm:COLLECTION is understood as an entity or class, defined by different XX elements, which themselves can contain their own attributes and child elements. Entities or classes differ from other complex elements therefore only in one point: Entities or classes contain at least one unique and unambiguous identifier which allows and supports the re-usability of those entities. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                
<!-- Mandatory elements -->
                <xs:element ref="dcddm:title" minOccurs="1" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:collectionDescriptionCreated" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="dcddm:description" minOccurs="1" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:hasContact" minOccurs="1" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:collectionType" minOccurs="1"/>
                <xs:element ref="dcddm:collectionDescriptionRights" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="dcddm:accessRights" minOccurs="1" maxOccurs="1"/>
                              
<!-- Content and item related elements -->
                <xs:element ref="dcddm:subject" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:spatial" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:temporal" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:collectionCreated" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:dateItemsCreated" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:itemType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:size" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:language" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:metadataEncodingScheme" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:itemEncodingScheme" minOccurs="0" maxOccurs="unbounded"/>         

<!-- Contextual elements -->
                <xs:element ref="dcddm:creator" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:owner" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:audience" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:isLocatedAt" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:hasPartOf" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:isPartOf" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:associatedCollection" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:associatedProject" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:provenance" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:acronym" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:identifier" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:providedIdentifier" minOccurs="0" maxOccurs="unbounded"/>
                
<!-- Collection policy elements -->
                <xs:element ref="dcddm:accrualMethod" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:accrualPolicy" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:accrualPeriodicity" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:itemsRights" minOccurs="0" maxOccurs="unbounded"/>

<!-- Service elements   -->
                <xs:element ref="dcddm:hasWebPage" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:hasOAI-PMH" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:hasREST-API" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:hasSPARQLEndpoint" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:hasOPAC" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:hasBEACON" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:hasReadingRoom" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:hasReproductionPossibilities" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="AGENT">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/Agent">
                Definition: A resource that acts or has the power to act. Examples of Agent include person, organization, and software agent.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: In the DARIAH Collection Description Data Model a dcddm:AGENT is understood as an entity or class, defined by XX elements, which themselves can contain their own attributes and child elements. Entities or classes differ from other complex elements therefore only in one point. Entities or classes contain at least one unique and unambiguous identifier which allows and supports the re-usability of those entities.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                    <xs:element ref="dcddm:agentType" minOccurs="1"/>
                    <xs:element ref="dcddm:name" minOccurs="1"/>
                    <xs:element ref="dcddm:forename" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element ref="dcddm:identifier" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element ref="dcddm:providedIdentifier" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element ref="dcddm:address" minOccurs="0" maxOccurs="1"/>
                    <xs:element ref="dcddm:email" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element ref="dcddm:hasWebPage" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element ref="dcddm:phoneNumber" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
   
<!--
    #############################################################
    #                    ELEMENT-DEFINITIONS                    #
    #############################################################
-->
    
    <xs:element name="title" type="dcddm:simpleLiteral">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/elements/1.1/title">
                Definition: A name given to the resource.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctitle">
                Comment: A value string must be provided.                
                To record titles in multiple languages, use multiple statements and in each statement associate the value string with the appropriate language tag. 
                Where an existing name is used, the value string should preserve the original wording, order and spelling of an existing name. Punctuation need not reflect the usage of the original. Subtitles should be separated from the title by a sequence of space-colon-space.
                The usage of several different languages in one titel element should be avoided if possible.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="identifier" type="dcddm:identifierValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/elements/1.1/identifier">
                Definition: An unambiguous reference to the resource within a given context.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: A collection identifier should be a URI, using a URI scheme that has been registered with IANA.                
                If the DCDDM is used in applications which help creating collection descriptions (i.e. DARIAH-DE Collection Registry), the application has to provide a valid URI.
                The attribute normdataRecord holds information about any normdata record the provided identifier is taken from.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Condition: Only if used in application which create their own identifieres. Otherwise this element should not be used.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="providedIdentifier" type="dcddm:identifierValues">
        <xs:annotation>
            <xs:documentation source="dcddm Data Model" xml:lang="en">
                Definition: An unambiguous reference to the resource within an already existing context like an institution or an application.
            </xs:documentation>
            <xs:documentation source="dcddm Data Model" xml:lang="en">
                Comment: In case your collection is already supplied with one ore more identifiers which you would like keep on using to maintain interoperability.
                The attribute normdataRecord holds information about any normdata record the provided identifier is taken from. 
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="collectionType" type="dcddm:collectionTypeValues">
        <xs:annotation>
            <xs:documentation source="http://purl.org/dc/elements/1.1/type">
                Definition: The nature or genre of the resource.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Use of standard vocabulary required. Provided values are based on DARIAH-DE suggestions.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="hasContact">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: Provides at least one possibility for potential users of the collection to get in contact with a person/organisation responsible for the collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Possible contact information might be an email address (recommended), a phone number, a web page (which must provide contact information) or an address.
                For multiple contact informations use multiple dcddm:hasContact statements.
                Each statement contains only one child element. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:choice>
                <xs:element ref="dcddm:email" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:hasWebPage" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:phoneNumber" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:address" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:AGENT" minOccurs="0" maxOccurs="1"></xs:element>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="description" type="dcddm:simpleLiteral">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermsabstract">
                Definition: A free text summary description of the collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermsabstract">
                Comment: A value string must be provided.              
                To record descriptions in multiple languages, use multiple statements and in each statement associate the value string with the appropriate language tag.
                Although a description might contain detailed subject-specific information, at least part of the description should be understandable by an end-user with no specialist knowledge of the subject area.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="acronym" type="xs:string">  
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Defintion: An acronym of the collection.  
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="size" type="xs:integer">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/extent">
                Definition: The size of the collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/extent">
                Comment: The size or extent of the collection should be described by providing the number of the collection`s items. If the exact number is unknown or the collection is still growing a rough estimation should be provided. 
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="language" type="dcddm:languageValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldclanguage">
                Definition: A language of the items in the collection.</xs:documentation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldclanguage">
                Comment: A value string must be provided; The value string should be a language name in the form of the three character code defined by ISO 639-3.
                To facilitate mappings and transformations of already existing collection descriptions the use of the two character code defined by ISO 639-1 is possible but ISO 639-3 is the recommended standard.
                Indicate the chosen standard through the attribute isoStandard 
                Where the collection covers multiple languages, a separate statement should be used for each language.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="itemType" type="dcddm:itemTypeValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#colclditemType ">
                Definition: The nature or genre of one or more items within the collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/documents/dcmi-type-vocabulary/#H7">
                Comment: The value strings are provided by DCMI Type Vocabulary.
                Where the collection includes items of multiple types, the significance of each dcddm:itemType for the collection is expressed through the order of the elements beginning with the most important one.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="collectionDescriptionRights" type="dcddm:rigthsValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldcrights">
                Definition: A statement of any rights held in/over the collection description.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="https://dev2.dariah.eu/wiki/x/cg9FAg">
                Comment: Provides information concerning the collection description and NOT the items of the collections. 
                The value strings are provided and maintained by DARIAH-DE.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="itemsRights" type="dcddm:rigthsValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldcrights ">
                Definition: A statement of any rights held in/over the collection´s items.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="https://dev2.dariah.eu/wiki/x/cg9FAg">
                Comment: Provides information concerning the collection´s items. 
                Where the collection includes items with different rights, use several dcddm:itemRights statements. The significance of each dcddm: itemRights is expressed through the order of the elements beginning with the most important one.
                Use of standard vocabulary required. Provided values are based on DARIAH-DE suggestions and maintained by DARIAH-DE.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="accessRights" type="dcddm:accessRightsValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermsaccessRights">
                Definition: A statement of any access restrictions placed on the collection.
            </xs:documentation>
            <xs:documentation xml:lang="de" source="dcddm Data Model">
                Use of standard vocabulary required. Provided values are based on DARIAH-DE suggestions and maintained by DARIAH-DE.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="accrualMethod" type="dcddm:accrualMethodsValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermsaccrualMethod">
                Definition:A fixed vocabulary naming methods by which items are added to a collection.</xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Recommended best practice is to use a value from a standard vocabulary provided and maintained by DARIAH-DE but based on http://dublincore.org/groups/collections/accrual-method/. 
                A value string must be provided.
                Where multiple methods apply, a separate statement should be used for each method. The importance of each dcddm:accrualMethod  statement is expressed by the applied order, beginning with the most important one.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="accrualPeriodicity" type="dcddm:accrualPeriodicityValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermsaccrualPeriodicity">
                Definition: A frequency with which items are added to a collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Recommended best practice is to use a value from a standard vocabulary provided and maintained by DARIAH-DE but based on http://dublincore.org/groups/collections/frequency/  
                A value string must be provided.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="accrualPolicy" type="dcddm:accrualPolicyValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermsaccrualPolicy">
                Definition: A policy governing the addition of items to a collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Recommended best practice is to use a value from a standard vocabulary provided and maintained by DARIAH-DE but based on http://dublincore.org/groups/collections/accrual-policy/ 
                A value string must be provided.
                Where multiple methods apply, a separate statement should be used for each method. The importance of each statement is expressed by the applied order, beginning with the most important one.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="provenance" type="dcddm:simpleLiteral">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermsprovenance">
                Definition: A statement of any changes in ownership and custody of the resource since its creation that are significant for its authenticity, integrity, and interpretation.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment:To record changes in ownership and custody in multiple languages, use multiple statements and in each statement associate the value string with the appropriate language tag xml:lang. 
                Use only one statement to report all changes, instead of one statement for each changement.
                Do not just note the name of places/persons or institution without providing some more information on the number of the concerning items or on the date the described changes took place.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="audience" type="dcddm:simpleLiteral">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/audience">
                Definition: A class of entity for whom the resource is intended or useful.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="subject">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldcsubject ">
                Definition: A subject or topic associated with the items in the collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Keywords or subject descriptors associated with items in the collection.
                The terms used indicate the subject matter of the collection.
                To enable homogenous descriptions the values of dcddm:subjects have to be taken from a controlled vocabulary, namely from GND, LCSH and DDC though the preferred vocabularies are GND and LCSH. 
                To avoid ambiguity the subjects must be referred by their specific identifiers. 
                To keep up the collection description´s human readability a label element should be provided. 
                To record multiple languages, use multiple labels.
                Where multiple keywords or subject descriptors are provided, a separate statement should be used for each keyword or descriptor.
                The importance of each statement is expressed by the applied order, beginning with the most important one.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:subjectValues" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation xml:lang="en" source="dcddm Data Model">
                            Definition: Contains the code, signature, URI, etc. matching the chosen encodingScheme-attribute.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="dcddm:label" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation xml:lang="en" source="dcddm Data Model">
                            Definition: Provides a human readable text of the chosen subject.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="spatial">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermsspatial ">
                Definition: An indicator of the spatial scope of the resource.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: To enable homogenous descriptions the values of dcddm:spatial have to be taken from a controlled vocabulary, namely from GND, GeoNames and Getty. Preferred are GeoNames and Getty because both provide gps coordinates. 
                To avoid ambiguity the locations must be referred by their specific identifiers. 
                To keep up the collection description´s human readability a label element should be provided. 
                To record multiple languages, use multiple labels.
                Where multiple spatial descriptors are provided, a separate statement should be used for each descriptor.
                The importance of each statement is expressed by the applied order, beginning with the most important one.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:spatialValues" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:label" minOccurs="1" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="temporal">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermstemporal">
                Definition: An indicator of the temporal scope of the resource.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: To enable homogenous descriptions the values of dcddm:temporal have to follow a syntax encoding scheme
                To keep up the collection description´s human readability a label element should be provided. 
                To record multiple languages, use multiple labels.
                Where multiple temporal descriptors are provided, a separate statement should be used for each descriptor.
                The importance of each statement is expressed by the applied order, beginning with the most important one. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:date" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="dcddm:label" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence> 
        </xs:complexType>
    </xs:element>
    <xs:element name="collectionDescriptionCreated" type="dcddm:periodeValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermscreated">
                Definition: A date of the creation of this collection description.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: To enable homogenous descriptions the values of dcddm:collectionDescriptionCreated have to follow a syntax encoding scheme.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="collectionCreated">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermscreated">
                Definition: A range of dates over which the collection was accumulated.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: To enable homogenous descriptions the values of dcddm:collectionCreated have to follow a syntax encoding scheme
                To keep up the collection description´s human readability a label element should be provided. 
                To record multiple languages, use multiple labels.
                Where multiple temporal descriptors are provided, a separate statement should be used for each descriptor.
                The importance of each statement is expressed by the applied order, beginning with the most important one.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:date" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:label" minOccurs="0" maxOccurs="1"/>
            </xs:sequence> 
        </xs:complexType>
    </xs:element>
    <xs:element name="dateItemsCreated">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#colclddateItemsCreated">
                Definition: A range of dates over which the individual items within the collection were created.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: To enable homogenous descriptions the values of dcddm:created have to follow a syntax encoding scheme
                To keep up the collection description´s human readability a label element should be provided. 
                To record multiple languages, use multiple labels.
                Where multiple temporal descriptors are provided, a separate statement should be used for each descriptor.
                The importance of each statement is expressed by the applied order, beginning with the most important one. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:date" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="dcddm:label" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence> 
        </xs:complexType>
    </xs:element>
    <xs:element name="creator">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldccreator">
                Definition: An entity who gathers (or gathered) the items in a collection together. resource.</xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Holds either a dcddm:AGENT or a reference to a dccdm:AGENT. 
                To record multiple creators, separate statements should be used. 
                The importance of each statement is expressed by the applied order, beginning with the most important one. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:AGENT" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="owner">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#colmarcrelOWN  ">
                Definition: An entity who has legal possession of the collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Holds either a dcddm:AGENT or a reference to a dccdm:AGENT. 
                To record multiple creators, seperate statements should be used. 
                The importance of each statement is expressed by the applied order, beginning with the most important one. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:AGENT" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="isLocatedAt">
        <xs:annotation>
            <xs:documentation source="http://dublincore.org/groups/collections/collection-application-profile/#colcldisLocatedAt" xml:lang="en">
                Definition: A location where the collection is held.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Holds a dcddm:address statement.
                To record multiple locations, separate statements should be used. 
                The importance of each statement is expressed by the applied order, beginning with the most important one.
                In case the collection´s item are digitized it is recommended to name the adress related with the person/organisation who has legal possession of the collection. Usually this person/organisation is also referenced by dcddm:owner.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:address" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="hasPartOf">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermshasPart">
                Definition:The described resource includes the referenced resource either physically or logically.
                A second collection contained within the current collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Holds either a dcddm:COLLECTION or a reference to a dccdm:COLLECTION. 
                To record multiple collections, separate statements should be used.  
                The importance of each statement is expressed by the applied order, beginning with the most important one.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:COLLECTION" minOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="isPartOf">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#coldctermsisPartOf">
                Definition: The described resource is a physical or logical part of the referenced resource.
                A second collection that contains the current collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Holds either a dcddm:COLLECTION or a reference to a dccdm:COLLECTION. 
                To record multiple collections, separate statements should be used.  
                The importance of each statement is expressed by the applied order, beginning with the most important one.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:COLLECTION" minOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="associatedCollection">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://dublincore.org/groups/collections/collection-application-profile/#colcldassociatedCollection">
                Definition: A second collection that is associated with the current collection.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:COLLECTION" minOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="metadataEncodingScheme" type="dcddm:encodingSchemeValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: Encoding scheme used for modeling the object´s metadata of the collection.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Holds the name of a well established and documented schema (e.g. oai_dc, TEI-Header).
                To record multiple schemes, separate statements should be used
                The importance of each statement is expressed by the applied order, beginning with the most important one.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="itemEncodingScheme" type="dcddm:encodingSchemeValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: Encoding scheme used for modeling the collection´s objects. 
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Holds the name of a well established and documented schema (e.g. LIDO, METS, TEI). 
                Provided values are based on DARIAH-DE suggestions.
                To record multiple schemes, separate statements should be used.
                The importance of each statement is expressed by the applied order, beginning with the most important one.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="itemEncodingSelfMadeScheme">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model"> 
                Definition: Describes and holds an reference to some self made Schema describing the collection´s objects. 
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Use this element for describing a e.g. MySQL schema, naming the used tables and field names in plain text and/or by providing a reference to the schema itself. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:label" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation xml:lang="en" source="dcddm Data Model">
                          Definition: Contains text describing the schema.
                        </xs:documentation>
                        <xs:documentation xml:lang="en" source="dcddm Data Model">
                            Definition: Use this label to name and maybe describe the classes/collections/documents/tables/fields/elements/attributes/properties etc. of your schema.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="dcddm:linkToSchema" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation source="dcddm Data Model" xml:lang="en">
                            Definition: A URI pointing to the schemas location. 
                        </xs:documentation>
                        <xs:documentation source="dcddm Data Model" xml:lang="en">
                            Comment: The URI could point e.g. to a MySQL schema export. 
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="linkToSchema" type="xs:anyURI">
        <xs:annotation>
            <xs:documentation source="dcddm Data Model" xml:lang="en">
                Definition: A URI pointing to the schemas location. 
            </xs:documentation>
            <xs:documentation source="dcddm Data Model" xml:lang="en">
                Comment: The URI could point e.g. to a MySQL schema export. 
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="hasOAI-PMH">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: The (metadata of the) collection´s item are accessible through OAI-PMH.
                </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: If the collection does not provide OAI-PMH ignore this element.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:baseURL" minOccurs="1"/>
                <xs:element ref="dcddm:OAISet" minOccurs="1"/>
                <xs:element ref="dcddm:listMetadataFormats" minOccurs="1" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:queryPolicies" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="hasREST-API">
        <xs:annotation>
            <xs:documentation source="dcddm Data Model" xml:lang="en">
                Definition: The (metadata of the) collection´s item are accessible via REST-API
            </xs:documentation>
            <xs:documentation source="dcddm Data Model" xml:lang="en">
                Comment: If the collection does not provide a REST-API this element should be ignored.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:baseURL" minOccurs="1"/>
                <xs:element ref="dcddm:supportedRESTMethods" minOccurs="1" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:itemEncodingScheme" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="dcddm:queryPolicies" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:furtherInfo" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="hasSPARQLEndpoint" type="xs:anyURI">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://en.wikipedia.org/wiki/SPARQL">
                Definition: The collection´s item are accessible via SPARQL-Endpoint. SPARQL is a recursive synonym for SPARQL Protocol and RDF Query Language. A SPARQL-Endpoint is a service that accepts SPARQL queries and returns results. The element provides the SPARQL-Endpoint´s URL.
            </xs:documentation>
            <xs:documentation source="dcddm Data Model" xml:lang="en">
                Comment: If the collection does not provide a SPARQL-Endpoint this element should be ignored.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="hasOPAC">
        <xs:annotation>
            <xs:documentation source="dcddm Data Model">
                Definition: The metadata of the collection´s item are accessible via OPAC (Online Public Access Catalogue).
            </xs:documentation>
            <xs:documentation source="dcddm Data Model">
                Comment: If the collection is not accessible through a SPARQL-Endpoint this element should be ignored.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:baseURL" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="dcddm:OPACCollectionIdentifier" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="hasBEACON">
        <xs:annotation>
            <xs:documentation source="http://gbv.github.io/beaconspec/beacon.html" xml:lang="en">
                Definition: BEACON is a data interchange format for large numbers of uniform links. The collection provides a BEACON-file.
            </xs:documentation>
            <xs:documentation source="dcddm Data Model" xml:lang="en">
                Comment: If the collection does not provide a BEACON-file this element should be ignored. 
                If the collection provides more than one BEACON-file use several statements for each file.    
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:baseURL" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="dcddm:beaconPrefix" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="hasReadingRoom">
        <xs:annotation>
            <xs:documentation source="dcddm Data Model" xml:lang="en">
                Definition: The collection´s items can be examined in a reading room.
            </xs:documentation>
            <xs:documentation source="dcddm Data Model" xml:lang="en">
                Comment: If there is not any reading room this statement should be ignored.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:address" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="dcddm:furtherInfo" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="hasReproductionPossibilities">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: The owner allows the users to make reproductions of the collection´s items or provides a reproduction service. The reproduction possibilities 
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: The reproduction possibilities can either be described in plain text or through a reference (URL) to an already existing description.
                If there are not any reproduction possibilities then this statement should be ignored. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:description" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="OPACCollectionIdentifier" type="xs:anyURI">
        <xs:annotation>
            <xs:documentation source="DLCAP Data Model" xml:lang="en">
                Definition: A URI (querystring) resolving in an OPAC`s list view of all items of the collection.
            </xs:documentation>
            <xs:documentation source="DLCAP Data Model" xml:lang="en"> 
                Comment: If the collection´s items (all of them) are not findable by one querystring this statement should be ignored. 
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Example: The items of the collection "Arithmetica", a collection of books are findabel by this querystring: "http://opac.lbs-braunschweig.gbv.de/DB=2/SET=2/TTL=32/CMD?ACT=SRCHA&amp;IKT=54&amp;TRM=sgn+sgn+a%3A%3Farithm%3F"
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="beaconPrefix">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://gbv.github.io/beaconspec/beacon.html">
                Definition: Specifies an URI pattern to construct sources identfiers.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="http://gbv.github.io/beaconspec/beacon.html">
                Comment: If no beacon PREFIX is specified this statement should be ignored.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="https://www2.landesarchiv-bw.de/ofs21/dienste/LABW-Personen-GND-beacon.txt">
                Example: http://d-nb.info/gnd/
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="supportedRESTMethods" type="dcddm:RESTMethodsValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: Provides information which REST-Methods are supported. The possible methods are: GET, POST, PUT, and DELETE.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Use one dcddm:supportedRESTMethods element for each supported method. 
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="furtherInfo" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: Provides any further information, either in plain text or in form of URL to a webpage containing query policies.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="baseURL" type="xs:anyURI">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: Provides an URL to the service´s entry site. 
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="OAISet" type="xs:string">
        <xs:annotation>
            <xs:documentation>
                Definition: Holds the name of an OAI-PMH Set. 
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="associatedProject">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: Names and shortly describes the project in which context the collection was created.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: It is recomended to provide a short description of the project and to provide a reference (URL) where further information can be found.
                To record information in multiple languages use several statements.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:description" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="dcddm:hasWebPage" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="listMetadataFormats" type="xs:string">
        <xs:annotation>
            <xs:documentation source="dcddm Data Model" xml:lang="en">
                Definition: Holds the name of the metadata format provided by the OAI-PMH.
                </xs:documentation>
            <xs:documentation source="dcddm Data Model" xml:lang="en">
                Comment: Use one dcddm:listMetadataFormats element for each provided format.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="queryPolicies" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: Provides information about query policies, either in plain text or in form of URL to a webpage containing query policies.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="address">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: Contains information related to a post address. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:street" minOccurs="0"/>                    
                <xs:element ref="dcddm:houseNumber" minOccurs="0"/>
                <xs:element ref="dcddm:postalCode" minOccurs="0"/>
                <xs:element ref="dcddm:place" minOccurs="0"/>
                <xs:element ref="dcddm:country" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="name" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://www.tei-c.org/release/doc/tei-p5-doc/de/html/ref-name.html ">
                Definition: A name or proper noun or noun phrase.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Should contain a person´s last or family name (surname) or the name of the described organisation.Should contain a person´s surname or the name of the described organisation. It is recommended to follow the naming conventions provided by the GND.
                
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="forename">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://www.tei-c.org/release/doc/tei-p5-doc/de/html/ref-forename.html">
                Definition: A forename, given or baptismal name.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Only used for persons. 
                In case of more than one forenames use only one statement to record all names. 
                It is recommended to follow the naming conventions provided by the GND.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="street" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: The street´s name of post address.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="houseNumber" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: The house number of the agent´s post address.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="postalCode" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: The postal code of the agent´s post address. 
            </xs:documentation>
            <xs:documentation>
                Comment: No use of country prefixes foreseen, e.g. use 38304 instead of D-38304.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="place">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: Holds information related to the place (city). 
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: The place should be linked to some normdata and provided with a human readable label. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:spatial"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="country">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: Names the country where the address is located 
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: The place should be linked to some normdata and provided with a human readable label. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dcddm:spatial"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="agentType" type="dcddm:agentTypeValues">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="http://purl.org/dc/elements/1.1/type">
                Definition: The nature or genre of the resource.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Use of standard vocabulary required. The provided values are 'person' and 'organisation'
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="email" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: Provides an email address realted with the AGENT.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Only provide email addresses when you can be sure that they will stay responsive for as long as the collection description is meant to be public or you have the resources to update them.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="hasWebPage" type="xs:anyURI">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: Provides a webpage related to the ressource.
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: Only provide webpages when you can be sure that they will stay online for as long as the collection description is meant to be public or you have the resources to keep them alive.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="phoneNumber" type="xs:string">
        <xs:annotation>
            <xs:documentation source="dcddm Data Model" xml:lang="en">
                Definition: Provides an phone number to a person/organisation capable of providing further information. 
            </xs:documentation>
            <xs:documentation source="dcddm Data Model" xml:lang="en">
                Comment: Follow the syntax encoding scheme provided by http://www.din-5008-richtlinien.de/telefonnummern.php. The provision of a country code (+49) is recommended. 
            </xs:documentation>
            <xs:documentation source="http://www.din-5008-richtlinien.de/telefonnummern.php" xml:lang="en">
                Example:  +49 5331 808-307
            </xs:documentation>
        </xs:annotation>
    </xs:element> 
    <xs:element name="label" type="dcddm:simpleLiteral">
        <xs:annotation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Definition: A label provides human readable information 
            </xs:documentation>
            <xs:documentation xml:lang="en" source="dcddm Data Model">
                Comment: To record information in multiple languages, use multiple labels and in each label associate the value string with the appropriate language tag.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="spatialValues">
        <xs:complexType mixed="true">
            <xs:attribute name="normdataRecord" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="TGN">
                            <xs:annotation>
                                <xs:documentation xml:lang="en" source="http://purl.org/dc/terms/TGN">
                                    Definition:	The set of places specified by the Getty Thesaurus of Geographic Names.  
                                </xs:documentation>
